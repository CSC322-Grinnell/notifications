<!DOCTYPE html>
<html>
  <head>
    <title>Head Start</title>
    <%= stylesheet_link_tag 'application' %>
    <%= javascript_include_tag 'application' %> 
    <%= csrf_meta_tags %>
  <head>
  <body>

    <%= render '/shared/header' %>

    <main>

      <% if flash[:notice] %>
        <div id="notice" class="message"> <%= flash[:notice] %> </div>
      <% elsif flash[:warning] %>
        <div id="warning" class="message"> <%= flash[:warning] %> </div>
      <% end %> 

      <%= yield %>
    </main>
    
  </body>
</html>